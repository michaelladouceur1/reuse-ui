{
	"name": "reuse-ui",
	"version": "0.1.6",
	"scripts": {
		"start": "react-scripts start",
		"dev": "npm run clean && nodemon",
		"test": "react-scripts test",
		"clean": "rm -rf dist",
		"build": "tsc && cp package.json README.md ./dist && cp -r ./src/styles ./dist/src",
		"npm-publish": "npm run build && npm publish ./dist"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"dependencies": {
		"@types/jest": "^29.5.0",
		"@types/node": "^16.7.13",
		"@types/react": "^18.0.0",
		"@types/react-dom": "^18.0.0",
		"react-scripts": "5.0.1"
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^14.0.0",
		"@testing-library/user-event": "^14.4.3",
		"css-loader": "^6.7.3",
		"jest": "^29.5.0",
		"jest-dom": "^4.0.0",
		"nodemon": "^2.0.22",
		"style-loader": "^3.3.2",
		"ts-jest": "^29.0.5",
		"ts-loader": "^9.4.2",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.2",
		"typescript-plugin-css-modules": "^5.0.0",
		"webpack": "^5.76.3",
		"webpack-cli": "^5.0.1"
	},
	"peerDependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	}
}
